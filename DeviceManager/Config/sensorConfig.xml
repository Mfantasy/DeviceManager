<Sensors>
  <sensor name="MXS1201" title="空气温湿度" sname="SeaBo_MXS1201">
    <field name="nodeid" alias="节点编号" realtime="false" history="false" />
    <field name="uid" alias="网关唯一号" realtime="false" history="false" />   
    <field name="humid" alias="湿度(%)" realtime="true" history="false" />
    <field name="humtemp" alias="温度(℃)" realtime="true" history="false" />
  </sensor>
  <sensor name="MXS1301" title="二氧化碳" sname="SeaBo_MXS1301">
    <field name="nodeid" alias="节点编号" realtime="false" history="false" />
    <field name="uid" alias="网关唯一号" realtime="false" history="false" />
    <field name="co2" alias="co2浓度(ppm)" realtime="true" history="false" />    
  </sensor>
  <sensor name="MXS1501" title="光照强度" sname="SeaBo_MXS1501">
    <field name="nodeid" alias="节点编号" realtime="false" history="false"/>
    <field name="uid" alias="网关唯一号" realtime="false" history="false"/>
    <field name="light" alias="太阳光照" realtime="true" history="false"/>
  </sensor>
  <sensor name="MXN820" title="节点状态" sname="SeaBo_MXN820_STATUS">
    <field name="nodeid" alias="节点编号" realtime="false" history="false"/>
    <field name="uid" alias="网关唯一号" realtime="false" history="false"/>
    <field name="battVol" alias="电池电压" realtime="true" history="false"/>
  </sensor>
  <!--测试用例-->
  <sensor name="MXS5000" title="Davis气象站" sname="SeaBo_MXS5000">
    <field name="nodeid" alias="节点编号" realtime="false" history="false" />
    <field name="uid" alias="网关唯一号" realtime="false" history="false" />
    <field name="port" alias="采集通道" realtime="false" history="false" />
    <field name="rain" alias="雨量(0.2mm)" realtime="true" history="false" />
    <field name="windspeed" alias="风速(m/s)" realtime="true" history="false" />
    <field name="winddirection" alias="风向(度)" realtime="true" history="false" />
    <field name="airtemp" alias="空气温度" realtime="true" history="false" />
    <field name="airhumid" alias="空气湿度" realtime="true" history="false" />
  </sensor>   
  <sensor name="MX8100" title="井下液位" sname="SCity_MX8100">
    <field name="waterLevel" alias="液位" realtime="true" history="true" />
  </sensor>
</Sensors>
